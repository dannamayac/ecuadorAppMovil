    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "REACT_APP_API_BASE_URL": "http://54.161.176.21:8000/api",
      "REACT_APP_LOGIN_ENDPOINT": "/User/login",
      "REACT_APP_LOGOUT_ENDPOINT": "/User/logout",
      "REACT_APP_INCOMES_LIST_ENDPOINT": "/Ingresos/listData",
